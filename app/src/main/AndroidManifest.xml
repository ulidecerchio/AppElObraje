<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="maravilla.equipo.appelobraje20">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HistoriaActivity"
            android:label="@string/title_activity_historia"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".OrientacionActivity"
            android:label="@string/title_activity_orientacion"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".GaleriaActivity"
            android:label="@string/title_activity_galeria"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ContactoActivity"
            android:label="@string/title_activity_contacto"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PrimarioActivity"
            android:label="@string/title_activity_primario"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>